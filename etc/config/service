
config igmp 'igmp'
	option snooping '1'
	option aging_time '120'
	option proxy '1'

config iptv 'iptv'
	option enabled '0'
	option vlanid '0'

config iptv-lan
	option vlanid '0'
	option port 'lan1'

config iptv-lan
	option port 'lan2'
	option vlanid '48'

config iptv-lan
	option vlanid '0'
	option port 'lan3'

config qos 'qos'
	option enable '0'

